{
  "name": "libraryws",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "nx": "nx",
    "ip": "npm i --legacy-peer-deps",
    "docs": "swagger-ui-watcher ./apidocs/main.docs.json -p 3335 --no-open > /dev/null 2>&1 &",
    "knex": "ts-node --project ./apps/library/src/app/db-config/tsconfig.json ./node_modules/.bin/knex --knexfile ./apps/library/src/app/db-config/knexfile.ts",
    "mig": "npm run knex migrate:latest",
    "mig:make": "npm run knex migrate:make -- -x ts",
    "mig:up": "ts-node --project ./apps/library/src/app/db-config/tsconfig.json ./node_modules/.bin/knex --knexfile ./apps/library/src/app/db-config/knexfile.ts migrate:up -- ",
    "mig:uup": "ts-node --project ./apps/library/src/app/db-config/tsconfig.json ./node_modules/.bin/knex --knexfile ./apps/library/src/app/db-config/knexfile.ts migrate:down ",
    "seed": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make -- -x ts",
    "seed:single": "ts-node --project ./apps/library/src/app/db-config/tsconfig.json ./node_modules/.bin/knex --knexfile ./apps/library/src/app/db-config/knexfile.ts seed:run --verbose --specific",
    "db:reset": "node ./etc/reset-db.js"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@willsoto/nestjs-objection": "^8.1.0",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "knex": "^2.3.0",
    "nestjs-slonik": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.6.0",
    "slonik": "^33.0.4",
    "slonik-utilities": "^2.0.1",
    "swagger-ui-express": "^4.6.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.3.0",
    "@nrwl/jest": "15.3.0",
    "@nrwl/linter": "15.3.0",
    "@nrwl/nest": "15.3.0",
    "@nrwl/node": "15.3.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "15.3.0",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
