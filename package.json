    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/js-yaml": "^4.0.5",
    "@willsoto/nestjs-objection": "^8.1.0",
    "acorn": "^8.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fp-ts": "^2.11.5",
    "fp-ts-std": "^0.15.0",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19",
    "json-refs": "^3.0.15",
    "knex": "^2.3.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.6.0",
